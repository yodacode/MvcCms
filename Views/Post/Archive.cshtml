@model IEnumerable<MvcCms.Models.Post>

@{
    ViewBag.Title = "Archive";
    Layout = null;
}

<h3>Archive</h3>

<ul class="archive">
    @foreach (var post in Model.OrderByDescending(p=> p.Date))
    {
        <li>
            <div class="month">@post.Date.ToString("MM")</div>
            <div class="year">@post.Date.ToString("yyyy")</div>
        </li>
    }
</ul>

<ul id="archive">
    
</ul>
